.unity-progress-bar__container {
    max-height: none;
    height: 100%;
    min-height: 0;
}

.progress-line {
    background-image: url("project://database/Assets/01.Works/KGH/02.Sprites/Line.png?fileID=21300000&guid=3e9dc4b87a12a1d44a86765951818305&type=3#Line");
    flex-grow: 1;
}

.line-container {
    flex-grow: 1;
    flex-direction: row;
    position: absolute;
    width: 100%;
    height: 100%;
    justify-content: space-between;
}
